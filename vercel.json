{
  "projects": [
    {
      "src": "client/client-side/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "client/client-side/dist"
      }
    },
    {
      "src": "server/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/client-side/$1"
    }
  ]
}
