{
    "builds": [
      {
        "src": "client/client-side/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "client/client-side/dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "client/client-side/$1"
      }
    ]
  }
